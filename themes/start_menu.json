{
    "label":{
        "font":{
            "name":"Helvetica",
            "size":"30",
            "regular_path":"fonts/Helvetica-Font/Helvetica.ttf",
            "bold_path":"fonts/Helvetica-Font/Helvetica-Bold.ttf",
            "italic_path":"fonts/Helvetica-Font/Helvetica-Oblique.ttf",
            "bold_italic_path":"fonts/Helvetica-Font/Helvetica-BoldOblique.ttf"
        }
    },
    "@image_button":{
        "colours":{
            "normal_bg":"rgba(0,0,0,0)",
            "hovered_bg":"rgba(0,0,0,0)",
            "disabled_bg":"rgba(0,0,0,0)",
            "selected_bg":"rgba(0,0,0,0)",
            "active_bg":"rgba(0,0,0,0)",
            "normal_border":"rgba(0,0,0,0)",
            "hovered_border":"rgba(0,0,0,0)",
            "disabled_border":"rgba(0,0,0,0)",
            "selected_border":"rgba(0,0,0,0)",
            "active_border":"rgba(0,0,0,0)"
        }
    },
    "@title":{
        "font":{
            "name":"Helvetica",
            "size":"80"
        },
        "colours":{
